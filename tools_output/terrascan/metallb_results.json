              "id": "AC_K8S_0070",
              "name": "nolivenessProbe",
              "shortDescription": {
                "text": "No liveness probe will ensure there is no recovery in case of unexpected errors"
              },
              "properties": {
                "category": "Security Best Practices",
                "severity": "LOW"
              }
            },
            {
              "id": "AC_K8S_0087",
              "name": "runAsNonRootCheck",
              "shortDescription": {
                "text": "Minimize Admission of Root Containers"
              },
              "properties": {
                "category": "Identity and Access Management",
                "severity": "HIGH"
              }
            },
            {
              "id": "AC_K8S_0099",
              "name": "MemoryRequestsCheck",
              "shortDescription": {
                "text": "Memory Request Not Set in config file."
              },
              "properties": {
                "category": "Security Best Practices",
                "severity": "MEDIUM"
              }
            },
            {
              "id": "AC_K8S_0064",
              "name": "securityContextUsed",
              "shortDescription": {
                "text": "Apply Security Context to Your Pods and Containers"
              },
              "properties": {
                "category": "Infrastructure Security",
                "severity": "MEDIUM"
              }
            },
            {
              "id": "AC_K8S_0046",
              "name": "privilegedContainersEnabled",
              "shortDescription": {
                "text": "Minimize the admission of privileged containers"
              },
              "properties": {
                "category": "Identity and Access Management",
                "severity": "HIGH"
              }
            },
            {
              "id": "AC_K8S_0073",
              "name": "appArmorProfile",
              "shortDescription": {
                "text": "AppArmor profile not set to default or custom profile will make the container vulnerable to kernel level threats"
              },
              "properties": {
                "category": "Identity and Access Management",
                "severity": "MEDIUM"
              }
            },
            {
              "id": "AC_K8S_0084",
              "name": "falseHostNetwork",
              "shortDescription": {
                "text": "Containers Should Not Share the Host Network Namespace"
              },
              "properties": {
                "category": "Infrastructure Security",
                "severity": "MEDIUM"
              }
            },
            {
              "id": "AC_K8S_0081",
              "name": "allowedVolumes",
              "shortDescription": {
                "text": "Some volume types mount the host file system paths to the pod or container, thus increasing the chance of escaping the container to access the host"
              },
              "properties": {
                "category": "Identity and Access Management",
                "severity": "MEDIUM"
              }
            },
            {
              "id": "AC_K8S_0097",
